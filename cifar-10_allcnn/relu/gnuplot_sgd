set title 'Loss on CIFAR-10 Convolutional subsampling trained with SGD'
set datafile separator ','
set terminal png size 800,600 enhanced font "Helvetica,15"
set output 'allcnn_relu_SGD_loss.png'
plot 'cifar-10_allcnn/relu/allcnn_relu_SGD.log.train' using 1:5 with line title 'training loss',\
     'cifar-10_allcnn/relu/allcnn_relu_SGD.log.test' using 1:5 with line title 'test loss'
set title 'Accuracy on CIFAR-10 Convolutional subsampling trained with SGD'
set datafile separator ','
set terminal png size 800,600 enhanced font "Helvetica,15"
set output 'allcnn_relu_SGD_accuracy.png'
plot 'cifar-10_allcnn/relu/allcnn_relu_SGD.log.train' using 1:4 with line title 'train accuracy',\
     'cifar-10_allcnn/relu/allcnn_relu_SGD.log.test' using 1:4 with line title 'test accuracy'
