set datafile separator ','
plot 'allcnn_relu_SGD.log.train' using 1:5 with line title 'training loss',\
     'allcnn_relu_SGD.log.test' using 1:5 with line title 'test loss'
set term x11 1
plot 'allcnn_relu_SGD.log.train' using 1:4 with line title 'train accuracy', \
     'allcnn_relu_SGD.log.test' using 1:4 with line title 'test accuracy'
set terminal png size 400,300 enhanced font "Helvetica,20"
set output 'allcnn_relu_SGD.png'
