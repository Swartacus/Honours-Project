set title 'Loss on ReLU MNIST Max pooling trained with SGD'
set datafile separator ','
set terminal png size 800,600 enhanced font "Helvetica,15"
set output 'plots/mnist_pooling/relu/poolcnn_relu_SGD_loss.png'
plot 'mnist_pooling/relu/poolcnn_relu_SGD.log.train' using 1:5 with line title 'training loss' lw 3,\
     'mnist_pooling/relu/poolcnn_relu_SGD.log.test' using 1:5 with line title 'test loss' lw 3
set title 'Accuracy on ReLU MNIST Max pooling trained with SGD'
set datafile separator ','
set terminal png size 800,600 enhanced font "Helvetica,15"
set output 'plots/mnist_pooling/relu/poolcnn_relu_SGD_accuracy.png'
plot 'mnist_pooling/relu/poolcnn_relu_SGD.log.train' using 1:4 with line title 'train accuracy',\
     'mnist_pooling/relu/poolcnn_relu_SGD.log.test' using 1:4 with line title 'test accuracy'


