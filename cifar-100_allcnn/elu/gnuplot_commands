set title 'Loss on ELU CIFAR-100 Convolutional subsampling trained with SGD'
set datafile separator ','
set terminal png size 800,600 enhanced font "Helvetica,15"
set output 'plots/cifar-100_allcnn/elu/allcnn_elu_SGD_loss.png'
plot 'cifar-100_allcnn/elu/allcnn_elu_SGD.log.train' using 1:6 with line title 'coarse label training loss',\
     'cifar-100_allcnn/elu/allcnn_elu_SGD.log.test' using 1:6 with line title 'coarse label test loss',\
     'cifar-100_allcnn/elu/allcnn_elu_SGD.log.train' using 1:7 with line title 'fine label training loss' ,\
     'cifar-100_allcnn/elu/allcnn_elu_SGD.log.test' using 1:7 with line title 'fine label test loss'
set title 'Accuracy on ELU CIFAR-100 Convolutional subsampling trained with SGD'
set datafile separator ','
set terminal png size 800,600 enhanced font "Helvetica,15"
set output 'plots/cifar-100_allcnn/elu/allcnn_elu_SGD_accuracy.png'
plot 'cifar-100_allcnn/elu/allcnn_elu_SGD.log.train' using 1:4 with line title 'coarse label train accuracy',\
     'cifar-100_allcnn/elu/allcnn_elu_SGD.log.test' using 1:4 with line title 'coarse label test accuracy',\
     'cifar-100_allcnn/elu/allcnn_elu_SGD.log.train' using 1:5 with line title 'fine label train accuracy',\
     'cifar-100_allcnn/elu/allcnn_elu_SGD.log.train' using 1:5 with line title 'fine label train accuracy'


