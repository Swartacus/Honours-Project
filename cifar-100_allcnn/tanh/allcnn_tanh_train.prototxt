layer {
  name: "data"
  type: "HDF5Data"
  top: "data"
  top: "label_coarse"
  top: "label_fine"
  hdf5_data_param {
    source: "../../../data/cifar-100/cifar_100_caffe_hdf5/train.txt"
    batch_size: 64
  }
}
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data"
  top: "conv1"
  convolution_param {
    num_output: 96
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "tanh1"
  type: "TanH"
  bottom: "conv1"
  top: "conv1"
}
layer {
  name: "conv2"
  type: "Convolution"
  bottom: "conv1"
  top: "conv2"
  convolution_param {
    num_output: 96
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "bn1"
  type: "BatchNorm"
  bottom: "conv2"
  top: "bn1"
}
layer {
  name: "tanh2"
  type: "TanH"
  bottom: "bn1"
  top: "bn1"
}
layer {
  name: "conv3"
  type: "Convolution"
  bottom: "bn1"
  top: "conv3"
  convolution_param {
    num_output: 96
    kernel_size: 3
    stride: 2
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "drop1"
  type: "Dropout"
  bottom: "conv3"
  top: "drop1"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "tanh3"
  type: "TanH"
  bottom: "drop1"
  top: "drop1"
}
layer {
  name: "conv4"
  type: "Convolution"
  bottom: "drop1"
  top: "conv4"
  convolution_param {
    num_output: 192
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "tanh4"
  type: "TanH"
  bottom: "conv4"
  top: "conv4"
}
layer {
  name: "conv5"
  type: "Convolution"
  bottom: "conv4"
  top: "conv5"
  convolution_param {
    num_output: 192
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "bn2"
  type: "BatchNorm"
  bottom: "conv5"
  top: "bn2"
}
layer {
  name: "tanh5"
  type: "TanH"
  bottom: "bn2"
  top: "bn2"
}
layer {
  name: "conv6"
  type: "Convolution"
  bottom: "bn2"
  top: "conv6"
  convolution_param {
    num_output: 192
    kernel_size: 3
    stride: 2
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "drop4"
  type: "Dropout"
  bottom: "conv6"
  top: "drop4"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "tanh6"
  type: "TanH"
  bottom: "drop4"
  top: "drop4"
}
layer {
  name: "conv7"
  type: "Convolution"
  bottom: "drop4"
  top: "conv7"
  convolution_param {
    num_output: 192
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "tanh7"
  type: "TanH"
  bottom: "conv7"
  top: "conv7"
}
layer {
  name: "conv8"
  type: "Convolution"
  bottom: "conv7"
  top: "conv8"
  convolution_param {
    num_output: 192
    kernel_size: 1
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "tanh8"
  type: "TanH"
  bottom: "conv8"
  top: "conv8"
}
layer {
  name: "conv9"
  type: "Convolution"
  bottom: "conv8"
  top: "conv9"
  convolution_param {
    num_output: 10
    kernel_size: 1
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "tanh9"
  type: "TanH"
  bottom: "conv9"
  top: "conv9"
}
layer {
  name: "pool"
  type: "Pooling"
  bottom: "conv9"
  top: "pool"
  pooling_param {
    pool: AVE
    global_pooling: true
  }
}
layer {
  name: "flatten"
  type: "Flatten"
  bottom: "pool"
  top: "flatten"
}
layer {
  name: "ip_c"
  type: "InnerProduct"
  bottom: "flatten"
  top: "ip_c"
  inner_product_param {
    num_output: 20
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "accuracy_f"
  type: "Accuracy"
  bottom: "ip_c"
  bottom: "label_coarse"
  top: "accuracy_f"
}
layer {
  name: "loss_c"
  type: "SoftmaxWithLoss"
  bottom: "ip_c"
  bottom: "label_coarse"
  top: "loss_c"
}
layer {
  name: "ip_f"
  type: "InnerProduct"
  bottom: "flatten"
  top: "ip_f"
  inner_product_param {
    num_output: 100
    weight_filler {
      type: "gaussian"
    }
  }
}
layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "ip_f"
  bottom: "label_fine"
  top: "accuracy"
}
layer {
  name: "loss_f"
  type: "SoftmaxWithLoss"
  bottom: "ip_f"
  bottom: "label_fine"
  top: "loss_f"
}
