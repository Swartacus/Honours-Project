set title 'Loss on LeakyReLU CIFAR-100 Convolutional subsampling trained with SGD'
set datafile separator ','
set terminal png size 800,600 enhanced font "Helvetica,15"
set output 'plots/cifar-100_allcnn/leakyrelu/allcnn_leakyrelu_SGD_loss.png'
plot 'cifar-100_allcnn/leakyrelu/allcnn_leakyrelu_SGD.log.train' using 1:5 with line title 'training loss' lw 3,\
     'cifar-100_allcnn/leakyrelu/allcnn_leakyrelu_SGD.log.test' using 1:5 with line title 'test loss' lw 3
set title 'Accuracy on LeakyReLU CIFAR-100 Convolutional subsampling trained with SGD'
set datafile separator ','
set terminal png size 800,600 enhanced font "Helvetica,15"
set output 'plots/cifar-100_allcnn/leakyrelu/allcnn_leakyrelu_SGD_accuracy.png'
plot 'cifar-100_allcnn/leakyrelu/allcnn_leakyrelu_SGD.log.train' using 1:4 with line title 'train accuracy',\
     'cifar-100_allcnn/leakyrelu/allcnn_leakyrelu_SGD.log.test' using 1:4 with line title 'test accuracy'


